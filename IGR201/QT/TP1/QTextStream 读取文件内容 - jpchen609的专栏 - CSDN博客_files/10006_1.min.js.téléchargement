oojs.define({name:"page",namespace:"",deps:{},sdk:{},adsExtention:[],resizeTimer:null,$page:function(){try{this.adClick("container")}catch(a){}},bindActionHandler:function(){},adClick:function(b){b=b||"container";var a=document.getElementById(b);var g=a.getElementsByTagName("a");for(var d=0;d<g.length;d++){var f=g[d].className;var c=g[d].id;if(f){f=f.toLowerCase();if(f==="gylogo"||f==="bdlogo"||f.substring(0,7)==="bd-logo"||c.indexOf("titleIconA")<0){continue}}this.bind(g[d],"click",this.onAdClick.proxy(this))}},onAdClick:function(c){e=this.formatEventObj(c);var g=e.target;while(g.tagName.toLowerCase()!=="a"&&g.tagName.toLowerCase()!=="body"){g=g.parentNode}var h=this.getAttr(g,"type");if(h==="phone"){var i=this.getAttr(g,"curl");var d=this.getAttr(g,"href");window.open(d,"_self");var k=document.createElement("iframe");if(i){k.src=i}k.style.display="none";document.body.appendChild(k);return this.stopEvent(c)}else{if(h==="message"){var p=navigator.userAgent;var o=this.getAttr(g,"msgCon");var b=this.getAttr(g,"msgNum");var f=o;var l=p.toLowerCase().match(/iphone/i)=="iphone";if(l){f=f.replace(/\?body/,";body")}var a=p.toLowerCase().match("baiduboxapp")==="baiduboxapp";var m=f.indexOf("=");var n=f.substring(0,m)+"="+encodeURIComponent(f.substring(m+1));if(a){var j=f.indexOf("body");n=n.substring(0,j-1)+" "+n.substring(j-1)}window.open(n,"_self");var k=document.createElement("iframe");var i=this.getAttr(g,"curl");if(i){k.src=i}k.style.display="none";document.body.appendChild(k);return this.stopEvent(c)}}},bind:function(a,c,b){if(a.addEventListener){a.addEventListener(c,b,false)}else{a.attachEvent("on"+c,b)}},getAttr:function(b,a){if(b&&b.getAttribute){return b.getAttribute(a)}else{return b[a]}},formatEventObj:function(a){a=a||window.event;a.target=a.target||a.srcElement;return a},stopEvent:function(a){if(a&&a.stopPropagation){a.stopPropagation()}else{window.event.cancelBubble=true}if(a&&a.preventDefault){a.preventDefault()}return false}});